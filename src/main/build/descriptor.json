{
  "package": {
    "name": "${project.name}",
    "repo": "maven",
    "subject": "fbacchella",
    "desc": "Snmpcodec is a resilient snmp parser and formater.",
    "website_url": "https://github.com/fbacchella/serializedlog4jappender",
    "issue_tracker_url": "https://github.com/fbacchella/serializedlog4jappender/issues",
    "vcs_url": "https://github.com/fbacchella/serializedlog4jappender.git",
    "github_use_tag_release_notes": false,
    "licenses": [
      "Apache License 2.0"
    ],
    "public_download_numbers": true,
    "public_stats": true
  },
  "version": {
    "name": "${project.version}"
  },
  "files": [
    {
      "includePattern": "target/(.*\\.jar)",
      "uploadPattern": "/fr/loghub/${project.name}/${project.version}/$1"
    },
    {
      "includePattern": "pom.xml",
      "uploadPattern": "/fr/loghub/${project.name}/${project.version}/${project.name}-${project.version}.pom"
    }
  ],
  "publish": true
}
