{
  "package": {
    "name": "${project.name}",
    "repo": "maven",
    "subject": "fbacchella",
    "desc": "A appender for log4j that serialize events instead of formating them",
    "website_url": "https://github.com/fbacchella/serializedlog4jappender",
    "issue_tracker_url": "https://github.com/fbacchella/serializedlog4jappender/issues",
    "vcs_url": "https://github.com/fbacchella/serializedlog4jappender.git",
    "github_use_tag_release_notes": false,
    "licenses": [
      "Apache License 2.0"
    ],
    "public_download_numbers": true,
    "public_stats": true
  },
  "version": {
    "name": "${project.version}"
  },
  "files": [
      {
        "includePattern": "target/deploy/(fr/loghub/serializedlog4jappender/${project.version}/.*)",
        "uploadPattern": "$1"
      }
  ],
  "publish": true
}
